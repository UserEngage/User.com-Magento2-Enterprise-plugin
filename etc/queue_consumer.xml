<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework-message-queue:etc/consumer.xsd">
    <consumer name="usercom.customer.sync"
              queue="usercom.customer.sync"
              handler="Usercom\Analytics\Model\CustomerSync::syncCustomerById"/>
    <consumer name="usercom.customer.login"
              queue="usercom.customer.login"
              handler="Usercom\Analytics\Model\CustomerSync::login"/>
    <consumer name="usercom.customer.register"
              queue="usercom.customer.register"
              handler="Usercom\Analytics\Model\CustomerSync::register"/>
    <consumer name="usercom.customer.newsletter"
              queue="usercom.customer.newsletter"
              handler="Usercom\Analytics\Model\CustomerSync::newsletter"/>
    <consumer name="usercom.catalog.product.event"
              queue="usercom.catalog.product.event"
              handler="Usercom\Analytics\Model\CatalogSync::productView"/>
    <consumer name="usercom.cart.product.add"
              queue="usercom.cart.product.add"
              handler="Usercom\Analytics\Model\CartSync::add"/>
    <consumer name="usercom.order.purchase"
              queue="usercom.order.purchase"
              handler="Usercom\Analytics\Model\CartSync::purchase"/>
    <consumer name="usercom.cart.product.remove"
              queue="usercom.cart.product.remove"
              handler="Usercom\Analytics\Model\CartSync::remove"/>
    <consumer name="usercom.cart.checkout"
              queue="usercom.cart.checkout"
              handler="Usercom\Analytics\Model\CartSync::checkout"/>
</config>
